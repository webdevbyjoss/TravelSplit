    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.5.2",
    "eslint": "^9.32.0",
