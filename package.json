{
  "name": "travelsplit",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Split travel expenses with friends and family",
  "keywords": [
    "travel",
    "expenses",
    "split",
    "pwa",
    "react"
  ],
  "author": "TravelSplit",
  "homepage": "https://travelsplit.app",
